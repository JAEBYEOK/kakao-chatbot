# OpenAI API 설정
OPENAI_API_KEY=your_openai_api_key_here

# 제주 관광 API 설정
JEJU_TOURISM_API_KEY=your_jeju_api_key_here
JEJU_TOURISM_API_URL=https://api.visitjeju.net

# 데이터베이스 설정
DATABASE_URL=sqlite:///data/navigation.db

# OSRM 서버 설정
OSRM_SERVER_HOST=localhost
OSRM_SERVER_PORT=5000

# 음성 처리 설정
STT_SERVICE_URL=http://localhost:8001
TTS_SERVICE_URL=http://localhost:8002

# 로깅 설정
LOG_LEVEL=INFO
LOG_FILE=logs/navigation_system.log

# 개발 환경 설정
ENVIRONMENT=development
DEBUG=true

# 지도 서비스 API 키
MAPBOX_API_KEY=your_mapbox_api_key_here
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# 캐시 설정
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=3600 